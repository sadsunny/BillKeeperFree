<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
     >
     
      <LinearLayout
        android:id="@+id/mLinearLayout1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:focusable="true"
        android:orientation="vertical"
        android:paddingBottom="6dp" >

        <TextView
            android:id="@+id/account_label"
            style="@style/TextAppearance.EditEvent_LabelSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="0dip"
            android:text="@string/account_label" >
         
          </TextView>
        <Button
            android:id="@+id/accountNa"
            style="@style/TextAppearance.EditEvent_SpinnerButton"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="-12dip"
            android:gravity="left|center_vertical"
            android:hint="@string/hint_account" 
            android:focusable="true"
            android:textColor="@color/text_black">
       
        </Button>
    </LinearLayout>
    
        <LinearLayout
        android:id="@+id/mLinearLayout1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:focusable="true"
        android:orientation="vertical"
        android:paddingBottom="6dp" >

        <TextView
            android:id="@+id/account_label"
            style="@style/TextAppearance.EditEvent_LabelSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="0dip"
            android:text="START DATE" >
         
          </TextView>
        <Button
            android:id="@+id/start_Button"
            style="@style/TextAppearance.EditEvent_SpinnerButton"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="-12dip"
            android:gravity="left|center_vertical"
            android:focusable="true"
            android:textColor="@color/text_black">
       
        </Button>
    </LinearLayout>
    
          <LinearLayout
        android:id="@+id/mLinearLayout1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:focusable="true"
        android:orientation="vertical"
        android:paddingBottom="6dp" >

        <TextView
            android:id="@+id/account_label"
            style="@style/TextAppearance.EditEvent_LabelSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="0dip"
            android:text="END DATE" >
         
          </TextView>
        <Button
            android:id="@+id/end_Button"
            style="@style/TextAppearance.EditEvent_SpinnerButton"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="-12dip"
            android:gravity="left|center_vertical"
            android:focusable="true"
            android:textColor="@color/text_black">
        </Button>
    </LinearLayout>
    
     <RelativeLayout
        android:id="@+id/toptextlayoput"
        android:layout_width="fill_parent"
        android:layout_height="50dp" >

        <TextView
            android:id="@+id/autopay_label"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="16dip"
            android:layout_marginRight="16dip"
            android:minHeight="24dip"
            android:paddingLeft="8dip"
            android:paddingRight="8dip"
            android:text="File Type"
            android:textAppearance="?android:attr/textAppearanceMedium" 
            android:textColor="@color/text_black"/>
        
        <ImageButton 
            android:id="@+id/csv_Button"
            android:layout_width="65.6dp"
            android:layout_height="30dp"
            android:layout_centerVertical="true"
            android:background="@drawable/csv_seletor"
            android:layout_toLeftOf="@+id/html_Button"
            android:layout_marginRight="0dp"
            />
        
        <Button 
            android:id="@+id/html_Button"
            android:layout_width="65.6dp"
            android:layout_height="30dp"
            android:layout_centerVertical="true"
            android:background="@drawable/html"
            android:layout_toLeftOf="@+id/pdf_Button"
            android:layout_marginRight="0dp"
            />
        
        
        <Button 
            android:id="@+id/pdf_Button"
            android:layout_width="65.6dp"
            android:layout_height="30dp"
            android:layout_centerVertical="true"
            android:background="@drawable/pdf"
            android:layout_alignParentRight="true"
            android:layout_marginRight="15dp"
            />
        
    </RelativeLayout>
          
    
</LinearLayout>
